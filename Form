Private Sub DELETE_Button_Click()
    For i = 1 To 25
    If Controls("TextBox" & i) = "" Then
        If i = 1 Then
            TextBox1.Value = ""
        Else
            Controls("TextBox" & (i - 1)) = ""
        End If
    End If
    Next
End Sub

Private Sub E_Button_Click()
    For i = 1 To 25
        If Controls("TextBox" & i) = "" And Controls("TextBox" & i).Visible = True Then
            Controls("TextBox" & i) = E_Button.Caption
            Exit For
    End If
    Next
End Sub

Private Sub ENTER_Button_Click()
    
    
    If TextBox1 <> "" And TextBox2 <> "" And TextBox3 <> "" And TextBox4 <> "" And TextBox5 <> "" Then
    Call Word(TextBox1.Value, TextBox2.Value, TextBox3.Value, TextBox4.Value, TextBox5.Value)
    For i = 1 To 5
        Controls("TextBox" & i).Enabled = False
        Controls("TextBox" & (5 + i)).Enabled = True
        Controls("TextBox" & i).BackColor = Color(i - 1)
        
    Next
    For i = 6 To 10
        Controls("TextBox" & i).Visible = True
    Next i
    End If

    If TextBox6 <> "" And TextBox7 <> "" And TextBox8 <> "" And TextBox9 <> "" And TextBox10 <> "" Then
        Call Word(TextBox6.Value, TextBox7.Value, TextBox8.Value, TextBox9.Value, TextBox10.Value)
        For i = 6 To 10
            Controls("TextBox" & i).Enabled = False
            Controls("TextBox" & i).BackColor = Color(i - 6)
        Next i
        For i = 11 To 15
        Controls("TextBox" & i).Visible = True
        Next i
    End If
    If TextBox11 <> "" And TextBox12 <> "" And TextBox13 <> "" And TextBox14 <> "" And TextBox15 <> "" Then
    Call Word(TextBox11.Value, TextBox12.Value, TextBox13.Value, TextBox14.Value, TextBox15.Value)
    For i = 11 To 15
        Controls("TextBox" & i).Enabled = False
        Controls("TextBox" & i).BackColor = Color(i - 11)
    Next
    For i = 16 To 20
        Controls("TextBox" & i).Visible = True
    Next i
   
    End If
    If TextBox16 <> "" And TextBox17 <> "" And TextBox18 <> "" And TextBox19 <> "" And TextBox20 <> "" Then
    Call Word(TextBox16.Value, TextBox17.Value, TextBox18.Value, TextBox19.Value, TextBox20.Value)
    
    For i = 16 To 20
        Controls("TextBox" & i).Enabled = False
        Controls("TextBox" & i).BackColor = Color(i - 16)
    Next
    For i = 21 To 25
        Controls("TextBox" & i).Visible = True
    Next i
    End If
    
    If TextBox21 <> "" And TextBox22 <> "" And TextBox23 <> "" And TextBox24 <> "" And TextBox25 <> "" Then
    Call Word(TextBox21.Value, TextBox22.Value, TextBox23.Value, TextBox24.Value, TextBox25.Value)
    For i = 21 To 25
        Controls("TextBox" & i).Enabled = False
        Controls("TextBox" & i).BackColor = Color(i - 21)
    Next
    End If

End Sub

