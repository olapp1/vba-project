Private Sub A_Button_Click()
    For i = 1 To 25
        If Controls("TextBox" & i) = "" And Controls("TextBox" & i).Visible = True Then
            Controls("TextBox" & i) = A_Button.Caption
            Exit For
    End If
    Next
End Sub

